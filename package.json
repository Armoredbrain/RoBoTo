{
  "name": "roboto_server",
  "version": "2.16.0",
  "description": "A chat bot to not feel lonely",
  "main": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": "18.16.0"
  },
  "scripts": {
    "build": "tsc --project ./",
    "husky:check": "node --require ts-node/register ./checkIfHuskyExists",
    "husky:setup": "node --require ts-node/register ./prepare",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --check **/*.{ts,json}",
    "prettier:fix": "prettier --write **/*.{ts,json}",
    "start": "yarn husky:check && nodemon -L ./src/index.ts",
    "swagger:bundle": "npx swagger-cli bundle ./src/swagger/baseApi.json --outfile ./src/swagger/swagger.json --type json && yarn prettier:fix",
    "test": "set NODE_ENV=test&& jest --detectOpenHandles --verbose",
    "test:coverage": "set NODE_ENV=test&& jest --detectOpenHandles --verbose --coverage",
    "test:watch": "set NODE_ENV=test&& jest --detectOpenHandles --silent --verbose --watch",
    "stryker": "npx stryker run --ignoreStatic"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^7.0.2",
    "@stryker-mutator/jest-runner": "^7.0.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.1",
    "@types/jsonwebtoken": "9.0.2",
    "@types/morgan": "1.9.4",
    "@types/node": "20.1.3",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.2",
    "mongodb-memory-server-core": "8.12.2",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "stryker-html-reporter": "^0.18.1",
    "stryker-jest-runner": "^1.4.1",
    "stryker-typescript": "^0.18.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "axios": "1.4.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-validator": "7.0.1",
    "helmet": "7.0.0",
    "jsonwebtoken": "9.0.0",
    "mongodb": "5.5.0",
    "mongoose": "7.1.1",
    "morgan": "1.10.0",
    "swagger-ui-express": "4.6.3",
    "ts-node": "^10.9.1",
    "winston": "3.8.2",
    "yaml": "2.2.2"
  },
  "keywords": [],
  "author": ""
}
